generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id    Int     @id @default(autoincrement())
  name  String
  email String? @unique
}

model posts {
  id      Int     @id @default(autoincrement())
  title   String?
  user_id Int?
}
